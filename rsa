#!/usr/bin/env python3

import sys

import sys

def is_prime(n):
    if n < 2:
        return False
    if n == 2:
        return True
    if n % 2 == 0:
        return False
    for i in range(3, int(n**0.5)+1, 2):
        if n % i == 0:
            return False
        return True

    def find_prime_factors(n):
        if n % 2 == 0:
            return (2, n//2)
        for i in range(3, int(n**0.5)+1, 2):
            if n % i == 0:
                if is_prime(i) and is_prime(n//i):
                    return (i, n//i)
                return None

            if len(sys.argv) != 2:
                print("Usage: rsa <file>")
sys.exit(1)

        filename = sys.argv[1]
        with open(filename, 'r') as f:
            n = int(f.readline().strip())
        if is_prime(n):
            print(f"{n}={n}*1")
        else:
            factors = find_prime_factors(n)
        if factors:
            print(f"{n}={factors[0]}*{factors[1]}")
